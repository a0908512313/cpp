{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "C/C++ Runner: Debug Session",
			"type": "cppdbg",
			"request": "launch",
			"args": [],
			"stopAtEntry": true,
			"externalConsole": true,
			"cwd": "c:/Users/user/Desktop/cpp",
			"program": "c:/Users/user/Desktop/cpp/build/Debug/outDebug",
			"MIMode": "gdb",
			"miDebuggerPath": "gdb",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				}
			]
		}
	]
}
